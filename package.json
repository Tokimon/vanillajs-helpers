{
  "name": "vanillajs-helpers",
  "version": "2.0.3",
  "description": "Collection of convenience code snippets (helpers) that aims to make it a little easier to work with vanilla JS",
  "author": "Toke Voltelen",
  "license": "MIT",
  "keywords": [
    "vanilla js",
    "helpers",
    "snippets",
    "utility",
    "modules",
    "typescript",
    "es6",
    "cjs"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "test": "cross-env TS_NODE_PROJECT=./tsconfig.test.json mocha --colors --require ts-node/register ./src/**/*.test.ts",
    "coverage": "nyc npm test -- --reporter progress",
    "build": "node ./tasks/build.js",
    "clean": "node ./tasks/clean.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "coveralls:win": "type ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "docs": "typedoc --options ./typedoc.json ./ts",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://Tokimon@github.com/Tokimon/vanillajs-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/Tokimon/vanillajs-helpers/issues"
  },
  "homepage": "https://github.com/Tokimon/vanillajs-helpers#readme",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.1",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.1",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "fs-extra": "^9.1.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "ts-node": "^9.1.1",
    "tslint-config-semistandard": "^8.0.1",
    "typedoc": "^0.20.30",
    "typedoc-plugin-markdown": "^3.6.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {}
}
