{
  "name": "vanillajs-helpers",
  "version": "1.2.0",
  "description": "Collection of convenience code snippets (helpers) that aims to make it a little easier to work with vanilla JS",
  "author": "Toke Voltelen",
  "license": "MIT",
  "keywords": [
    "vanilla js",
    "helpers",
    "event",
    "camelCase",
    "isObject",
    "capitalize",
    "hash"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "test": "cross-env TS_NODE_PROJECT=./cjs mocha --compilers ts:ts-node/register ./test/**/*.spec.ts",
    "build": "tsc && tsc -p ./cjs && tsc -p ./es5",
    "jsdoc": "node ./tasks/jsdoc.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "coveralls:win": "type ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Tokimon@github.com/Tokimon/vanillajs-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/Tokimon/vanillajs-helpers/issues"
  },
  "homepage": "https://github.com/Tokimon/vanillajs-helpers#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.13.1",
    "fs-extra": "^4.0.1",
    "glob": "^7.1.2",
    "glob-promise": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.13.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1",
    "yargs": "^8.0.2"
  },
  "dependencies": {
    "@types/chai-as-promised": "^7.1.0",
    "@types/sinon-chai": "^2.7.29",
    "cross-env": "^5.1.1"
  }
}
